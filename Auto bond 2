local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

-- ğŸ—‚ï¸ Äáº·t tÃªn folder chá»©a Bond náº¿u cÃ³
local bondFolder = workspace:FindFirstChild("Bonds") or workspace
local bondName = "Bond" -- TÃªn cá»§a Part Ä‘áº¡i diá»‡n Bond

-- âš™ï¸ Cáº¥u hÃ¬nh
local teleportDelay = 0.5 -- Thá»i gian giá»¯a má»—i láº§n dá»‹ch chuyá»ƒn

-- ğŸ” Teleport Ä‘áº¿n táº¥t cáº£ Bond
local function teleportAllBonds()
	for _, obj in ipairs(bondFolder:GetDescendants()) do
		if obj:IsA("BasePart") and obj.Name == bondName then
			-- Teleport nhÃ¢n váº­t tá»›i vá»‹ trÃ­ Bond
			hrp.CFrame = obj.CFrame + Vector3.new(0, 3, 0)
			task.wait(teleportDelay)
		end
	end
end

-- ğŸ‘‰ Gá»i hÃ m nÃ y Ä‘á»ƒ báº¯t Ä‘áº§u
teleportAllBonds()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Sonnek1203/chuotnekhub/refs/heads/main/Auto%20bond"))()
